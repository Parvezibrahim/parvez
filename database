#include <stdio.h>

int main()
{int a;
float b;
char c;

    printf("enter the integer  value");
    scanf("%d",&a);
    printf("\nThe integer you have entered is %d",a);
    printf("\n enter the float value");
    scanf("\n %f",&b);
    printf("\n The float you entered is %f",b);
    printf("\n enter the character value ");
    scanf("\n%c",&c);
    printf("\n The entered values is char %c",c);
    

    return 0;
    }
    
