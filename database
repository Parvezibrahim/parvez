    #include <stdio.h>

int main()
{int a,b;
    printf("enter the two values");
    scanf("%d%d",&a,&b);
    if(a>b)
    printf("%d is greater than the %d",a,b);
    else
    printf("%d is greater than the %d",b,a);
    

    return 0;
}
